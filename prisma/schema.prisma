generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Staff {
  id   Int         @id @default(autoincrement())
  name String
  CNIC String      @unique
  Phone  String            @unique
  Designation String
  Address String
  Joining_date String   
}
model complain {
  id   Int         @id @default(autoincrement())
  name String
  CNIC String      @unique
  Phone  String            @unique
  Compalain_type  String
  Address String
  Description String 
  Status String 
}